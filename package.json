  "bugs": {
    "url": "https://github.com/gwatts/go-coverage-action/issues"
  },
  "homepage": "https://github.com/gwatts/go-coverage-action#readme",
  "dependencies": {
    "@actions/core": "^1.7.0",
    "@actions/github": "^5.0.0",
    "execa": "^6.1.0"
  },
  "devDependencies": {
    "@vercel/ncc": "^0.33.3",
    "eslint": "^8.15.0"
  },
  "volta": {
    "node": "16.14.0"
  }
